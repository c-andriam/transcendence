meta {
  name: User favorites asc sorted by time
  type: http
  seq: 5
}

get {
  url: {{baseUrl}}{{myFavPrefix}}?page=1&limit=10&sortBy=prepTime&sortOrder=asc
  body: none
  auth: bearer
}

params:query {
  page: 1
  limit: 10
  sortBy: prepTime
  sortOrder: asc
}

headers {
  x-gateway-api-key: {{gatewayKey}}
}

auth:bearer {
  token: {{bearerToken}}
}

settings {
  encodeUrl: true
  timeout: 0
}
