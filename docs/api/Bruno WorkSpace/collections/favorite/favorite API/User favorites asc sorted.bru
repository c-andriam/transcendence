meta {
  name: User favorites asc sorted
  type: http
  seq: 4
}

get {
  url: {{baseUrl}}{{myFavPrefix}}?page=1&limit=5&sortBy=title&sortOrder=asc
  body: none
  auth: bearer
}

params:query {
  page: 1
  limit: 5
  sortBy: title
  sortOrder: asc
}

headers {
  x-gateway-api-key: {{gatewayKey}}
}

auth:bearer {
  token: {{bearerToken}}
}

settings {
  encodeUrl: true
  timeout: 0
}
