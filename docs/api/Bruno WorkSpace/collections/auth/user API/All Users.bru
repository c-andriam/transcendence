meta {
  name: All Users
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}{{userPrefix}}users
  body: none
  auth: none
}

headers {
  x-gateway-api-key: {{gatewayKey}}
}

assert {
  res.status: eq 200
  res.body.status: eq success
  res.body.data: isArray
  res.body.data[0].isEmailVerified: eq true
}

settings {
  encodeUrl: true
  timeout: 0
}
