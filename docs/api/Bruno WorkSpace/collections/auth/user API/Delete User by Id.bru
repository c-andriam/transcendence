meta {
  name: Delete User by Id
  type: http
  seq: 5
}

delete {
  url: {{baseUrl}}{{userPrefix}}{{userId}}
  body: none
  auth: bearer
}

headers {
  x-gateway-api-key: {{gatewayKey}}
}

auth:bearer {
  token: {{bearerToken}}
}

assert {
  res.status: eq 200
  res.body.status: eq success
  res.body.data: isJson
  res.body.data.isEmailVerified: eq true
}

settings {
  encodeUrl: true
  timeout: 0
}
